<div id="editProfile">
<h2> Update Profile</h2>
<%= form_for(@user) do |fields|%>
	<%= render 'editProfileDetails', f:fields%>
	<div class="button_align">
		<ul>
			<li><%= fields.submit "Submit", class: 'submit_button' %></li>
			<li><%= link_to "Cancel", :back, class: 'submit_button' %></li>
		</ul>
	</div>
	<% end%>
</div>